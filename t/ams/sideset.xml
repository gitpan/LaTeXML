      <equation xml:id="S0.Ex13">
        <Math mode="display" xml:id="S0.Ex13.m1" tex="{}^{b}_{a}\sum\nolimits^{d}_{c}x" text="(^ b _ a (sum ^ d) _ c)@(x)">
          <XMath>
