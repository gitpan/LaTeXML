              </XMApp>
              <XMTok role="UNKNOWN" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S0.Ex8">
        <Math mode="display" xml:id="S0.Ex8.m1" tex="\sideset{{}_{a}{}^{b}}{{}_{c}{}^{d}}{\sum}x" text="(_ a ^ b sum _ c) ^ d * x">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp scriptpos="mid">
                <XMTok role="SUPERSCRIPTOP" scriptpos="post6"/>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post5"/>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="pre3"/>
                    <XMApp>
                      <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                      <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
