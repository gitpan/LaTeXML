  <paragraph xml:id="S0.SS0.SSS0.Px2">
    <title>Classic</title>
    <para xml:id="S0.SS0.SSS0.Px2.p1">
      <equation xml:id="S0.Ex10">
        <Math mode="display" xml:id="S0.Ex10.m1" tex="{}_{a}^{b}\sum_{c}^{d}x" text="(_ a ^ b (sum _ c) ^ d)@(x)">
          <XMath>
            <XMApp>
              <XMApp scriptpos="mid">
                <XMTok role="SUBSCRIPTOP" scriptpos="pre2"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                  <XMApp scriptpos="mid">
                    <XMTok role="SUPERSCRIPTOP" scriptpos="mid2"/>
                    <XMApp scriptpos="mid">
                      <XMTok role="SUBSCRIPTOP" scriptpos="mid2"/>
                      <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                      <XMTok role="UNKNOWN" font="italic">c</XMTok>
                    </XMApp>
                    <XMTok role="UNKNOWN" font="italic">d</XMTok>
                  </XMApp>
                  <XMTok role="UNKNOWN" font="italic">b</XMTok>
                </XMApp>
                <XMTok role="UNKNOWN" font="italic">a</XMTok>
              </XMApp>
              <XMTok role="UNKNOWN" font="italic">x</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S0.Ex11">
        <Math mode="display" xml:id="S0.Ex11.m1" tex="{}^{b}_{a}\sum^{d}_{c}x" text="(^ b _ a (sum ^ d) _ c)@(x)">
