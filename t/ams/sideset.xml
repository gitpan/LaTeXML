          <XMath>
            <XMApp>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="pre3"/>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="pre3"/>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="pre3"/>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="pre2"/>
                      <XMApp>
                        <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                        <XMApp>
                          <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                          <XMApp>
                            <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                            <XMTok mathstyle="display" meaning="sum" role="SUMOP">∑</XMTok>
                            <XMTok role="UNKNOWN" font="italic">f</XMTok>
                          </XMApp>
                          <XMTok role="UNKNOWN" font="italic">g</XMTok>
                        </XMApp>
                        <XMTok role="UNKNOWN" font="italic">e</XMTok>
